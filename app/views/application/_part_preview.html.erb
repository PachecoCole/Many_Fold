<% if can_preview?(part.file_format) %>
  <canvas
    id="preview-part-<%= part.id %>"
    data-preview
    data-preview-url="<%= library_model_part_path(library, model, part, part.file_format.to_sym)%>"
    data-format="<%= part.file_format %>"
    data-y-up="<%= part.y_up %>"
  ></canvas>
<% end %>
